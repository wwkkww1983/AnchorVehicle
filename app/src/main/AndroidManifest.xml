            android:name="com.baidu.lbsapi.API_KEY"
            android:value="03Hn3IGulwT7IZUz06mH4kXtockrx36d" />

        <receiver android:name=".receiver.MyReceiver" >
            <intent-filter>
                <action android:name="ELITOR_CLOCK" />
            </intent-filter>
        </receiver>
